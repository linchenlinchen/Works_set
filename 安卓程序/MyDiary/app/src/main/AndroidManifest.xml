<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.mydiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fatcat"
        android:label="FatCat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.RegisterActivity" />
        <activity android:name=".main.HomePageActivity" />
        <activity android:name=".main.MemoActivity"
            android:launchMode="singleTask"/>
        <activity
            android:name=".main.MemoContentActivity"
            android:theme="@style/Theme.AppCompat.Dialog"></activity>
        <activity android:name=".main.EditActivity"></activity>
    </application>

</manifest>